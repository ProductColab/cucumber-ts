{
  "default": {
    "format": [
      "@cucumber/pretty-formatter",
      [
        "html",
        "cucumber-report.html"
      ]
    ],
    "formatOptions": {
      "snippetInterface": "synchronous"
    },
    "requireModule": [
      "tsx/cjs",
      "tsconfig-paths/register"
    ],
    "require": [
      "features/**/*.ts"
    ]
  }
}